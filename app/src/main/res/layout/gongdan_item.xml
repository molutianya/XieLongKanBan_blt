            android:gravity="center"
            android:maxLines="1"
            android:text="工单编号"
            android:textColor="#000"
            android:textSize="14dp"
            />

        <TextView
            android:id="@+id/cpbh"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginLeft="1dp"
            android:background="#AFEEEE	"
            android:gravity="center"
