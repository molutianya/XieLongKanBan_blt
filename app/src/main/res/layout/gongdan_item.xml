            android:gravity="center"
            android:text="计划交货日期"
            android:textColor="#000"
            android:textSize="14dp"

            />

        <TextView
            android:id="@+id/gdbh"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginLeft="1dp"
